/dts-v1/;
/plugin/;

/ {
	overlay-name = "UEFI Boot order pxe default";

	fragment@0 {
		target-path = "/";
		board_config {
			sw-modules = "uefi";
		};

		__overlay__ {
			firmware {
				uefi {
					variables {
						gNVIDIATokenSpaceGuid {
							DefaultBootPriority {
								data = "pxev4";
								locked;
							};
						};
					};
				};
			};
		};
	};
};
